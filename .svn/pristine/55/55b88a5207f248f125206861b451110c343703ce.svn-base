{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": true,
		"noImplicitAny": false,
		"baseUrl": ".",
		"paths": {
			"app/*": ["./App/*"]
		}
	},
	"filesGlob": [
		"**/*.ts",
		"**/**/*.ts",
		"**/**/**/*.ts"
	],
	"compileOnSave": true
}
